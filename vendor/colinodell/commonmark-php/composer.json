{
    "name": "colinodell/commonmark-php",
    "type": "library",
    "description": "Markdown parser for PHP based on the CommonMark spec",
    "keywords": ["markdown","parser","commonmark"],
    "homepage": "https://github.com/colinodell/commonmark-php",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Colin O'Dell",
            "email": "colinodell@gmail.com",
            "homepage": "http://www.colinodell.com",
            "role": "Lead Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "symfony/options-resolver": "~2.5.6"
    },
    "require-dev": {
        "erusev/parsedown": "~1.0",
        "jgm/CommonMark": "0.12",
        "michelf/php-markdown": "~1.4",
        "phpunit/phpunit": "~4.3"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "jgm/CommonMark",
                "version": "0.12",
                "dist": {
                    "url": "http://spec.commonmark.org/0.12/spec.txt",
                    "type": "file"
                }
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "ColinODell\\CommonMark\\": "src/",
            "ColinODell\\CommonMark\\Tests\\": "tests/"
        }
    }
}
